<td class='game-team1'>
  <span class='<%= game.team1_style_class %>'>
    <%= link_to game.team1.title, live_game_path(game.id) %>
  </span>
  <span>
    <%= "(#{game.team1.title2})" if game.team1.title2.present? %>
  </span>

  <%= image_tag( "logos/24x24/#{game.team1.key}.png" ) if Rails.application.assets.find_asset( "logos/24x24/#{game.team1.key}.png" ).present? %>
</td>
